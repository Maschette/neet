cap_cols <- function(data, vals, lims) {
data[[vals]] <- ifelse(
# is column vals > column lims?
data[[vals]] > data[[lims]],
# if yes, return lims value
data[[lims]],
# if no, return vals value
data[[vals]])
data
}
